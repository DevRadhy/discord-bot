{
  "name": "hub-bot",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=16.6.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm-run-all -p start:*",
    "start:server": "node dist/index.js",
    "dev": "npm-run-all -p dev:*",
    "dev:server": "tsnd --inspect --respawn --transpile-only --ignore-watch node_modules -r dotenv/config src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "discord-api-types": "^0.37.5",
    "discord.js": "^13.2.0",
    "dotenv": "^10.0.0",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
